<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="BrowserStack Test Automation Regresion Suite"
	thread-count="4" parallel="tests" verbose="3">

	<test name="Login Page Test windows 10 - firefox">
		<parameter name="os" value="Windows" />
		<parameter name="os_version" value="10" />
		<parameter name="browser" value="Firefox" />
		<parameter name="browser_version" value="80.0" />
		<classes>
			<class name="com.mytests.SauceTest" />
		</classes>
	</test>

	<test name="Login Page Test on macOS - chrome">
		<parameter name="os" value="OS X" />
		<parameter name="os_version" value="Catalina" />
		<parameter name="browser" value="Chrome" />
		<parameter name="browser_version" value="85.0" />
		<classes>
			<class name="com.mytests.SauceTest" />
		</classes>
	</test>
	
</suite>